<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>Checkers</title>
<link rel="stylesheet" type="text/css" href="/css/style.css">
</head>
<body>
<h1.star>1STaR 3je37di 37Lo37go367 M3o736nolin;9</h1>
<h1>Star Wars Checkers FTW! </h1>
<nav>
  <a href="/">Home Page</a>
  <a href="/checkers">Checkerboard</a>
</nav>
<div style = "text-align:center;">
  <table class = "center">
    <% var bg, getsPiece = false, url %>
    <% for(var i = 1; i < 9; i++) { %>
    <tr class="tr<%= i % 2 %>">
      <% for(var j = 1; j < 9; j++) { %>
      <% if(i%2===1){ %>
      <% bg = (j%2===1) ? 'white' : 'black' %>
      <% }else{ %>
      <% bg = (j%2===1) ? 'black' : 'white' %>
      <% } %>
      <td class="td<%= j%2 %>"></td>
      <% } %>
    </tr>
    <% } %>
  </table>
</div>
<footer>
  <small>&copy 2014 Melanie L. Fryman</small>
</footer>
</body>
</html>
